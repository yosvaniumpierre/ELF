@using Telerik.Web.Mvc.UI
<!DOCTYPE html>
<html>
<head>
    @(Html.Telerik().StyleSheetRegistrar()
                          .DefaultGroup(group => group
                                      .Add("telerik/telerik.examples.min.css")
                                      .Add("telerik/telerik.common.css")
                                      .Add("telerik/telerik.simple.css")
                                      .Add("telerik/telerik.rtl.css")
                              .Combined(false)
                              .Compress(true))
                         )
    <title>@ViewBag.Title</title>
    @*<link href="@Url.Content("~/Content/telerik/telerik.forest.css")" rel="stylesheet" type="text/css" />*@
    @*<script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>*@
</head>

<body>
    @RenderBody()
</body>
</html>
